{% extends 'base.html.twig' %}

{% block javascripts %}
    <script src="{{ asset('assets/js/select.js') }}"></script>
{% endblock %}

{% block title %}
    {{ project.name }} - TaskLinker
{% endblock %}

{% block header %}
    <h1>{{ project.name }}</h1>
{% endblock %}

{% block body %}
    {{ form_start(form) }} 
        {{ form_row(form.name) }}
        {{ form_row(form.users) }}
        <button class="button button-submit">Modifier</button>
    {{ form_end(form) }}
{% endblock %}